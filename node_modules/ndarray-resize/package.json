{
  "name": "ndarray-resize",
  "version": "0.2.0",
  "description": "resize images in pure javascript without native dependencies",
  "author": "ismay",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint:js": "eslint '**/*.js'",
    "lint:prettier": "prettier --list-different '**/*.js'"
  },
  "publishConfig": {
    "access": "public"
  },
  "greenkeeper": {
    "ignore": [
      "assert-dir-equal",
      "coveralls",
      "eslint",
      "eslint-config-airbnb-base",
      "eslint-config-prettier",
      "eslint-plugin-import",
      "get-pixels",
      "husky",
      "jest",
      "lint-staged",
      "prettier",
      "rimraf",
      "save-pixels",
      "stream-to-promise"
    ]
  },
  "devDependencies": {
    "assert-dir-equal": "^1.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.13.0",
    "get-pixels": "^3.3.2",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.12.0",
    "rimraf": "^2.6.2",
    "save-pixels": "^2.3.4",
    "stream-to-promise": "^2.2.0"
  },
  "dependencies": {
    "zeros": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
